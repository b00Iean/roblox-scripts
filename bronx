if workspace:FindFirstChild('Safezone') then 
	workspace.Safezone:Destroy()
    workspace.Safezone:Destroy()
end

while wait() do
    if getgenv().settings['GARBAGE_FARM'] == true then 
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-1203, 3, -66)
        wait(.1)
        fireproximityprompt(workspace["Job 1"].DontDeleteTheseObjects.GetJobPart.ProximityPrompt)
        wait(.3)
        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, workspace["Job 1"].DontDeleteTheseObjects.TargetPart, 0)
        firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, workspace["Job 1"].DontDeleteTheseObjects.TargetPart, 1)
        wait(.1)
    end
    if getgenv().settings['BOX_FARM'] == true then 
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-801, 3, -176)
        wait(.2)
        fireclickdetector(workspace["Lower Garage Boxes"].ClickDetector)
        wait(.2)
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-875, 4, -255)
        wait(.2)
        fireclickdetector(workspace.MafiaMember.ClickDetector)
		wait(.2)
    end
end
